package com.msl.lists;

import java.util.ArrayList;

public class CollectionTest {

	public static void main(String[] args) {
		ArrayList<String> locations = new ArrayList<String>();
		locations.add("Hyd");
		locations.add("Blore");
		locations.add("Delhi");
		
		System.out.println("Locations: " + locations);
		
		

	}

}
